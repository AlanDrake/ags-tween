  Expect.EqualsInt("TintGreen", oHiRes.TintGreen, 0);

  oHiRes.Tint(255, 255, 255, 100, 100);
  oHiRes.TweenTintBlue(0.5, 0);
  Expect.EqualsInt("TintBlue", oHiRes.TintBlue, 0);

  oHiRes.Tint(255, 0, 0, 100, 100);
